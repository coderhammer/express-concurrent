  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run playground",
      "request": "launch",
      "runtimeArgs": ["run", "playground"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "console": "integratedTerminal"
    },
    {
      "name": "Run tests",
      "request": "launch",
      "runtimeArgs": ["run", "test"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "console": "integratedTerminal"
    }
  ]
}
